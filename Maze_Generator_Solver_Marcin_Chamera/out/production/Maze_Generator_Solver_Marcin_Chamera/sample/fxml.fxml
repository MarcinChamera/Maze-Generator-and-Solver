<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>

<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="100" vgap="10">
    <Canvas fx:id="canvas" GridPane.columnIndex="0" GridPane.rowSpan="10" width="620" height="620" onMousePressed="#mousePressed"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="0" text="makeMaze" onAction="#makeMaze" minWidth="100"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="1" text="ChooseStart" onAction="#setStart" minWidth="100"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="2" text="ChooseEnd" onAction="#setEnd" minWidth="100"/>
    <Button GridPane.columnIndex="1" GridPane.rowIndex="3" text="solveMaze" onAction="#solveMaze" minWidth="100"/>
</GridPane>